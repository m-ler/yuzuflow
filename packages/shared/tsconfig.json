{
	"extends": "./../../tsconfig.json",
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "./dist",
		"allowSyntheticDefaultImports": true,
	},
	"ts-node": {
		"esm": true
	},
}